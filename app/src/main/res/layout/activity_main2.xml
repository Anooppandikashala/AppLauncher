<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/samsu"
    tools:context="com.example.anoop.applauncher.Main2Activity">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="anooppkrishnan96@gmail.com"
        android:layout_marginTop="400dp"
        android:id="@+id/textView1"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic" />
    <!--android:layout_alignParentLeft="true"-->
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/copy"
        android:layout_marginTop="430dp"
        android:id="@+id/textView3"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        />
    <!--android:layout_alignParentLeft="true"-->
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Developer"
        android:layout_marginTop="210dp"
        android:id="@+id/textView2"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18dp"
        android:textStyle="normal|bold|italic" />
    <!--android:layout_alignParentLeft="true"-->
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="ANOOP"
        android:layout_marginTop="240dp"
        android:id="@+id/textView4"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="32dp"
        android:textStyle="normal|bold|italic" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="GEC Palakkad"
        android:layout_marginTop="285dp"
        android:id="@+id/textView5"
        android:textAlignment="center"
        android:textColor="?attr/textColorAlertDialogListItem"
        android:textSize="22dp"
        android:textStyle="normal|bold|italic" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageViewx"
        android:layout_width="match_parent"
        android:layout_height="138dp"
        android:src="@drawable/anoop12"
        android:layout_marginBottom="100dp"
        android:layout_marginTop="70dp"


        app:civ_border_width="2dp"
        app:civ_border_color="#FF000000"/>

    <TextView
        android:text="AM Launcher"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:textColor="#454aee"
        android:id="@+id/textView" />

</RelativeLayout>
